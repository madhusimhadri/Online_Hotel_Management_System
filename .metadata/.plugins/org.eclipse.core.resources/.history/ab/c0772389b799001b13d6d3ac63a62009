package com.hotel.Model;

import java.util.Date;

public class Invoice {
	
	private String companyName;
	private String guestId;
	private String guestName;
	private String guestMobile;
	private int numberOfPeople;
	private String bookedRoomType;
	private long roomPricePerNight;
	private int nights;
	private Date bookingDate;
	private float tax;
	private float totalBill;
	private String issuedBy;
	
	public Invoice()
	{
		
	}

	public Invoice(String companyName, String guestId, String guestName, String guestMobile, int numberOfPeople,
			String bookedRoomType, long roomPricePerNight, int nights, Date bookingDate, float tax, float totalBill,
			String issuedBy) {
		super();
		this.companyName = companyName;
		this.guestId = guestId;
		this.guestName = guestName;
		this.guestMobile = guestMobile;
		this.numberOfPeople = numberOfPeople;
		this.bookedRoomType = bookedRoomType;
		this.roomPricePerNight = roomPricePerNight;
		this.nights = nights;
		this.bookingDate = bookingDate;
		this.tax = tax;
		this.totalBill = totalBill;
		this.issuedBy = issuedBy;
	}


	public String getCompanyName() {
		return companyName;
	}


	public void setCompanyName(String companyName) {
		this.companyName = companyName;
	}


	public String getGuestId() {
		return guestId;
	}


	public void setGuestId(String guestId) {
		this.guestId = guestId;
	}


	public String getGuestName() {
		return guestName;
	}


	public void setGuestName(String guestName) {
		this.guestName = guestName;
	}


	public String getGuestMobile() {
		return guestMobile;
	}


	public void setGuestMobile(String guestMobile) {
		this.guestMobile = guestMobile;
	}


	public int getNumberOfPeople() {
		return numberOfPeople;
	}


	public void setNumberOfPeople(int numberOfPeople) {
		this.numberOfPeople = numberOfPeople;
	}


	public String getBookedRoomType() {
		return bookedRoomType;
	}


	public void setBookedRoomType(String bookedRoomType) {
		this.bookedRoomType = bookedRoomType;
	}


	public long getRoomPricePerNight() {
		return roomPricePerNight;
	}


	public void setRoomPricePerNight(long roomPricePerNight) {
		this.roomPricePerNight = roomPricePerNight;
	}


	public int getNights() {
		return nights;
	}


	public void setNights(int nights) {
		this.nights = nights;
	}


	public Date getBookingDate() {
		return bookingDate;
	}


	public void setBookingDate(Date bookingDate) {
		this.bookingDate = bookingDate;
	}


	public float getTax() {
		return tax;
	}


	public void setTax(float tax) {
		this.tax = tax;
	}


	public float getTotalBill() {
		return totalBill;
	}


	public void setTotalBill(float totalBill) {
		this.totalBill = totalBill;
	}


	public String getIssuedBy() {
		return issuedBy;
	}


	public void setIssuedBy(String issuedBy) {
		this.issuedBy = issuedBy;
	}
	
	
	
}
